
<!DOCTYPE html>
<html ng-app="webapp" ng-strict-di ng-cloak>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">

    <!-- Place favicon.ico and apple-touch-icon.png in the root directory -->
    <link rel="shortcut icon" type="image/ico" href="~/images/logo.png" />

    <title>Fíchame!!</title>
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.8.2/css/all.css" rel="stylesheet" />
    <link href='http://fonts.googleapis.com/css?family=Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>

    <!-- Primary vendor style -->
    @*<link href="~/Plugins/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Plugins/fontawesome/css/font-awesome.min.css" rel="stylesheet" />*@

    <script src="~/Plugins/angular/angular-1.5.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@8"></script>
    <script src="~/Scripts/umd/popper.js"></script>
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/respond.js"></script> 
    <script src="~/Plugins/sweetalert2/sweetalert2.min.js"></script>

    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.2.4/jquery.min.js" integrity="sha256-BbhdlvQf/xTY9gja0Dq3HiwQF8LaCRTXxZKRutelT44=" crossorigin="anonymous"></script>

    @* <script src="~/Plugins/jquery/jquery-ui.min.js"></script>
        <script src="~/Plugins/jquery/jquery.ui.touch-punch.min.js"></script>
        <script src="~/Plugins/lodash/dist/lodash.min.js"></script>
            <link href="~/Plugins/bootstrap-markdown/bootstrap-markdown.min.css" rel="stylesheet" />
            <script src="~/Plugins/bootstrap-markdown/markdown.js"></script>
            <script src="~/Plugins/bootstrap-markdown/bootstrap-markdown.js"></script>
            <script src="~/Plugins/angular-bootstrap/ui-bootstrap.js"></script>
            <script src="~/Plugins/angular-bootstrap/ui-bootstrap-tpls.js"></script>*@
    <link href="~/Plugins/angular-ui-clock/angular-clock.css" rel="stylesheet" />



    <!-- DEFAULT style -->
    @Styles.Render("~/bundles/plugins/css")
    @Styles.Render("~/Content/css")

    <!-- Add local styles, mostly for plugins css file -->
    @if (IsSectionDefined("Styles"))
    {@RenderSection("Styles", required: false)}
</head>
<body ng-controller="mainCtrl as vm">
    <!--[if lt IE 7]>
    <p class="alert alert-danger">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience.</p>
    <![endif]-->
    @Html.Partial("_Header")


    <div class="container-fluid body-content h-100 vw-100 mb-1">

        @RenderBody()







        @Html.Partial("_Footer")
    </div>
    <!-- Directorio Raiz del Proyecto para las llamadas $http -->
    <script>var webroot = "@Url.Content("~/")";</script>

    <!-- Section for main scripts render -->
    @*@Scripts.Render("~/bundles/angular/js")*@


    @Scripts.Render("~/bundles/bootstrap/js")
    @Scripts.Render("~/bundles/plugins/js")
    @Scripts.Render("~/bundles/webApp/js")
    
   
    
    @RenderSection("scripts", required: false)

</body>
</html>
